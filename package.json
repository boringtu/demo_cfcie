{
	"name": "chat_for_customer_ie",
	"version": "1.0.0",
	"description": "基于 grunt 的自动化构建工具",
	"path": {
		"node_modules": "./node_modules/",
		"assets": "./dist/assets/",
		"image": "./images/",
		"src": {
			"path": "./src/",
			"scripts": "./src/scripts/",
			"styles": "./src/styles/",
			"templates": "./src/templates/",
			"images": "./src/images/",
			"fonts": "./src/fonts/"
		},
		"dist": {
			"path": "./dist/",
			"js": "./dist/js/",
			"tempJs": "./dist/tempJs/",
